{
	"name": "kirbi-discord",
	"description": "adds Discord support to Kirbi",
	"version": "0.1.0",
	"license": "Apache-2.0",
	"readme": "README.md",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Richardson-Media-House/kirbi-discord.git"
	},
	"author": "Nate Richardson <hello@naterichardson.com> (https://naterichardson.com)",
	"contributors": [
		"Matt Carroll <admin@develogix.com> (https://develogix.com)",
		"Joseph Moran III <joseph.moraniii@gmail.com> (https://josephmoran.co)"
	],
	"main": "discord-support.js",
	"dependencies": {
		"bufferutil": "^3.0.1",
		"chalk": "^2.1.0",
		"discord.js": "^11.1.0",
		"erlpack": "hammerandchisel/erlpack",
		"libsodium-wrappers": "^0.5.2",
		"node-opus": "0.2.6",
		"opusscript": "0.0.3",
		"sodium": "^2.0.1",
		"uws": "^0.14.1"
	},
	"devDependencies": {
		"xo": "^0.18.2"
	},
	"xo": {
		"plugins": [
			"unicorn"
		],
		"rules": {
			"linebreak-style": "off",
			"no-prototype-builtins": "warn",
			"radix": "warn",
			"guard-for-in": "warn", 
			"import/no-unresolved": "off",
			"camelcase": "off",
			"no-negated-condition": "warn",
			"object-curly-spacing": [
				"error",
				"always",
				{
					"arraysInObjects": false
				}
			]
		}
	},
	"scripts": {
		"test": "xo"
	}
}